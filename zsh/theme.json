{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#29315A",
    "prompt": "#7FD5EA"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:background",
          "foreground": "#bb9af7",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "\ue5ff {{ .Path }}",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "background": "p:background",
          "foreground": "cyan",
          "leading_diamond": " \ue0b6",
          "style": "diamond",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        },
        {
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "leading_diamond": " \ue0b6",
          "foreground": "#86DC3D",
          "background": "p:background",
          "properties": {
            "shell": "zsh",
            "command": "pgrep -P $(ps -fp $PPID | awk \"/$PPID/\"' { print $3 } ') | wc -l | awk '{print $1-1}'"
          },
          "template": "{{ if ne .Output \"-1\" }}\uf827 {{ .Output }}{{ end }}",
          "type": "command"
        },
        {
          "background": "p:background",
          "foreground": "#E4F34A",
          "leading_diamond": " \ue0b6",
          "style": "diamond",
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }} {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "p:background",
          "foreground": "#7FD5EA",
          "leading_diamond": " \ue0b6",
          "style": "diamond",
          "template": "\ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "go"
        },
        {
          "background": "p:background",
          "foreground": "lightBlue",
          "leading_diamond": " \ue0b6",
          "style": "diamond",
          "template": "\ue620 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "lua"
        },
        {
          "background": "p:background",
          "foreground": "#ff9e64",
          "leading_diamond": " \ue0b6",
          "style": "diamond",
          "template": "\uE7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "rust"
        },
        {
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "leading_diamond": " \ue0b6",
          "foreground": "#e64d43",
          "background": "p:background",
          "template": " {{ if le .Percentage 10 }}{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}%{{ end }} ",
          "properties": {
            "discharging_icon": "\uf579 ",
            "not_charging_icon": "\uf579 ",
            "charging_icon": "\uf585 ",
            "charged_icon": "\uf585 "
          },
          "type": "battery"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:background",
          "foreground": "p:prompt",
          "foreground_templates": [
            "{{if gt .Code 0}}#db4b4b{{end}}"
          ],
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\u276f",
          "trailing_diamond": "\ue0b4",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:prompt",
    "template": "<p:background>\ue0b6</><,p:background>\u276f</><p:background>\ue0b4</> "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "gray",
    "template": "<p:background>\ue0b6</><,p:background>></><p:background>\ue0b4</> "
  },
  "final_space": true,
  "version": 2
}
